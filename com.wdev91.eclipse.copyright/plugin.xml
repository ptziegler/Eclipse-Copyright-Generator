<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.eclipse.ui.preferencePages.Workbench"
            class="com.wdev91.eclipse.copyright.preferences.CopyrightPreferencePage"
            id="com.wdev91.eclipse.copyright.copyrightPreferencePage"
            name="%prefsCopyright_label">
      </page>
      <page
            category="com.wdev91.eclipse.copyright.copyrightPreferencePage"
            class="com.wdev91.eclipse.copyright.preferences.HeadersPreferencePage"
            id="com.wdev91.eclipse.copyright.headerPreferencePage"
            name="%prefsHeader_label">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            class="com.wdev91.eclipse.copyright.preferences.ProjectCopyrightPreferencePage"
            id="com.wdev91.eclipse.copyright.projectCopyrightPreferencePage"
            name="%prefsCopyright_label">
         <enabledWhen>
            <adapt
                  type="org.eclipse.core.resources.IProject">
            </adapt>
         </enabledWhen>
      </page>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>

</plugin>
